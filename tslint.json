{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest"
  ],
  "jsRules": {},
  "rules": {
    "align": false,
    "ordered-imports": [
      true,
      {
        "import-sources-order": "any",
        "named-imports-order": "any"
      }
    ],
    "interface-name": [
      true,
      "never-prefix"
    ],
    "class-name": true,
    "comment-format": false,
    "curly": false,
    "eofline": true,
    "forin": true,
    "indent": [
      true,
      "spaces",
      4
    ],
    "label-position": true,
    "max-line-length": [
      true,
      {
        "limit": 140,
        "ignore-pattern": "^\\s*\\*\\s?\\|"
      }
    ],
    "no-angle-bracket-type-assertion": false,
    "member-access": false,
    "member-ordering": [
      false,
      "static-before-instance"
    ],
    "max-classes-per-file": [
      false,
      1
    ],
    "no-arg": true,
    "no-bitwise": false,
    "no-console": [
      true,
      "debug",
      "info",
      "time",
      "timeEnd",
      "trace"
    ],
    "no-construct": true,
    "no-debugger": true,
    "no-duplicate-variable": true,
    "no-empty": false,
    "no-eval": true,
    "no-inferrable-types": false,
    "no-shadowed-variable": true,
    "no-string-literal": false,
    "no-switch-case-fall-through": true,
    "no-trailing-whitespace": true,
    "no-unused-expression": true,
    "no-var-keyword": true,
    "no-default-export": true,
    "object-literal-sort-keys": false,
    "jsdoc-format": false,
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ],
    "array-type": [
      false,
      "array"
    ],
    "one-line": [
      true,
      "check-open-brace",
      "check-catch",
      "check-else",
      "check-whitespace"
    ],
    "quotemark": [
      true,
      "single"
    ],
    "radix": false,
    "semicolon": [
      true,
      "never"
    ],
    "triple-equals": [
      true,
      "allow-null-check"
    ],
    "typedef-whitespace": [
      true,
      {
        "call-signature": "nospace",
        "index-signature": "nospace",
        "parameter": "nospace",
        "property-declaration": "nospace",
        "variable-declaration": "nospace"
      }
    ],
    "variable-name": false,
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-separator",
      "check-type"
    ],
    "trailing-comma": [
      false,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "never",
          "typeLiterals": "ignore"
        },
        "singleline": "never"
      }
    ],
    "unified-signatures": false,
    "no-implicit-dependencies": false,
    "no-submodule-imports": false,
    "no-object-literal-type-assertion": false
  },
  "rulesDirectory": []
}